<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <!--********************* -->
  <!--********************* -->
  <!-- ELEMENTO CONTENITORE -->
  <!--********************* -->
  <!--********************* -->
  <xs:element name="FIELD_DATA">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="PROCESSING_DATA_SECTION" minOccurs="0"/>
      </xs:sequence>
      <xs:attribute name="MSG_ID" type="SPECIAL_MESSAGE_IDENTIFIER"/>
      <xs:attribute name="MSG_DT" type="DATETIME" use="required"/>
      <xs:attribute name="CISNUM" type="INTEGER_7" use="optional"/>
      <xs:attribute name="MODCOD" type="INTEGER_3" use="optional"/>
      <xs:attribute name="CHANUM" type="STRINGA_ALN_7" use="optional"/>
      <xs:attribute name="SEQNUM" type="INTEGER_5" use="optional"/>
      <xs:attribute name="SEQBIW" type="INTEGER_BIW" use="optional"/>
      <xs:attribute name="LB__ID" type="INTEGER_4" use="optional"/>
      <xs:attribute name="LINCOD" type="INTEGER_2" use="optional"/>
      <xs:attribute name="PRGNUM" type="INTEGER" use="optional"/>
      <xs:attribute name="LINDES" type="STRINGA_GENER" use="optional"/>
      <xs:attribute name="SUBGROUP_DESCRIPTION" type="STRINGA_GENER" use="optional"/>
    </xs:complexType>
  </xs:element>
  <!--*************************** -->
  <!--*************************** -->
  <!-- SEZIONE PER LA GESTIONE DELLA TRACEABILITY -->
  <!--*************************** -->
  <!--*************************** -->
  <xs:element name="TRACEABILITY_SECTION">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="TRACEABILITY_OPERATION" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TRACEABILITY_OPERATION">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="CURRENT_ELEMENT"/>
        <xs:element ref="PREVIOUS_ELEMENT" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CURRENT_ELEMENT">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="SPECIAL_PN_SERIAL_CODE">
          <xs:attribute name="PNCODE" type="SPECIAL_PNCODE" use="required"/>
          <xs:attribute name="M_OR_B" type="ELENCO_1" use="required"/>
          <xs:attribute name="OPCODE" type="SPECIAL_OPERATION_CODE" use="required"/>
          <xs:attribute name="OPERATOR" type="STRINGA_GENER" use="optional"/>
          <!--Nuovo campo OPtoolID -->
          <xs:attribute name="OPTOOL_ID" type="STRINGA_GENER" use="optional"/>
          <!--Nuovo campo OPtoolID -->
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="PREVIOUS_ELEMENT">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="SPECIAL_PN_SERIAL_CODE">
          <xs:attribute name="PNCODE" type="SPECIAL_PNCODE" use="required"/>
          <xs:attribute name="M_OR_B" type="ELENCO_1" use="required"/>
          <xs:attribute name="OPCODE" type="SPECIAL_OPERATION_CODE" use="required"/>
          <xs:attribute name="OPERATOR" type="STRINGA_GENER" use="optional"/>
          <!--Nuovo campo OPtoolID -->
          <xs:attribute name="OPTOOL_ID" type="STRINGA_GENER" use="optional"/>
          <!--Nuovo campo OPtoolID -->
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <!--************************************************ -->
  <!--************************************************ -->
  <!-- SEZIONE PER LA GESTIONE DEI LOTTI DI LASTRATURA -->
  <!--************************************************ -->
  <!--************************************************ -->
  <xs:element name="SUBCOMPONENT_MANAGEMENT_SECTION">
    <xs:complexType>
      <xs:attribute name="SUBCOMPONENT_ID" type="STRINGA_GENER" use="optional"/>
      <xs:attribute name="SUBCOMPONENT_DESCRIPTION" type="STRINGA_GENER" use="optional"/>
      <!-- 	
			Il sottocomponente permette senza alcun tipo di validazione di associare dati ricevuti in precedenza ad un certo CIS 
			Se per il lotto che si riceve esiste giC  una associazione ad un CIS, i dati vengono legati automaticamente.
			Diversamente gli xml ricevuti vengono "parcheggiati" in attesa che il lotto venga associato al CIS tramite operazioni successive.
			In nessun caso la gestione del lotto corrisponde ad una operazione di traceability o di part verification.
			L'oggetto "subcomponent" viene direttamente legato al CIS.
			-->
    </xs:complexType>
  </xs:element>
  <!--****************************************************************** -->
  <!--****************************************************************** -->
  <!-- SEZIONE PER LA GESTIONE DATI DI PROCESSO: LAVORAZIONI E CONTROLLI -->
  <!--****************************************************************** -->
  <!--****************************************************************** -->
  <xs:element name="PROCESSING_DATA_SECTION">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="TRACED_OPERATION" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TRACED_OPERATION">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="SINGLE_ITEM" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="OPCODE" type="SPECIAL_OPERATION_CODE" use="required"/>
      <xs:attribute name="OPTYPE" type="ELENCO_5" use="required"/>
      <xs:attribute name="OPCYCLE" type="INTEGER" use="optional"/>
      <xs:attribute name="OPTOOL_ID" type="STRINGA_GENER" use="optional"/>
      <xs:attribute name="OP_RESULT" type="ELENCO_2" use="required"/>
      <xs:attribute name="OPERATOR" type="STRINGA_GENER" use="optional"/>
      <xs:attribute name="OPDATE" type="DATETIME" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="PROCESSING_CHECK_SECTION">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="CHECKED_DATA" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="CHECKED_DATA">
    <xs:complexType>
      <!--Nuovo sezione ERROR LIST -->
      <!--<xs:sequence>-->
      <!--<xs:element ref="ERROR_LIST" minOccurs="0" maxOccurs="unbounded"/>-->
      <!--</xs:sequence>-->
      <!--Nuovo sezione ERROR LI\ST -->
      <xs:simpleContent>
        <xs:extension base="ELENCO_2">
          <xs:attribute name="OPCODE" type="SPECIAL_OPERATION_CODE" use="required"/>
          <xs:attribute name="OPTYPE" type="ELENCO_6" use="required"/>
          <xs:attribute name="OPTOOL_ID" type="STRINGA_GENER" use="optional"/>
          <xs:attribute name="OPERATOR" type="STRINGA_GENER" use="optional"/>
          <!--Nuovo campo TRY_ID -->
          <xs:attribute name="TRY_ID" type="INTEGER" use="optional"/>
          <!--Nuovo campo TRY_ID -->
          <!--Nuova gestione dei componenti ERROR LIST -->
          <xs:attribute name="STATISTIC_CODE" type="STRINGA_GENER" use="optional"/>
          <xs:attribute name="STATISTIC_DESCRIPTION" type="STRINGA_GENER" use="optional"/>
          <xs:attribute name="STATISTIC_VALUE" type="STRINGA_GENER" use="optional"/>
          <!--Nuova gestione dei componenti ERROR LIST -->
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="SINGLE_ITEM">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="SINGLE_OP_RESULT" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="ITEM_ID" type="INTEGER" use="required"/>
      <xs:attribute name="ITEM_DESC" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="ITEM_RESULT" type="ELENCO_2" use="required"/>
      <xs:attribute name="OPERATOR" type="STRINGA_GENER" use="optional"/>
      <!--Nuovo campo OPtoolID -->
      <xs:attribute name="OPTOOL_ID" type="STRINGA_GENER" use="optional"/>
      <!--Nuovo campo OPtoolID -->
    </xs:complexType>
  </xs:element>
  <xs:element name="SINGLE_OP_RESULT">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="MEASUREMENT" maxOccurs="unbounded"/>
        <!--Nuovo campo ErrorList -->
        <xs:element ref="ERROR_LIST" minOccurs="0" maxOccurs="unbounded"/>
        <!--Nuovo campo ErrorList -->
      </xs:sequence>
      <xs:attribute name="TRY_ID" type="INTEGER" use="required"/>
      <xs:attribute name="TRY_RESULT" type="ELENCO_2" use="required"/>
      <!--Nuovo campo OPerator -->
      <xs:attribute name="OPERATOR" type="STRINGA_GENER" use="optional"/>
      <!--Nuovo campo OPerator -->
      <!--Nuovo campo OPtoolID -->
      <xs:attribute name="OPTOOL_ID" type="STRINGA_GENER" use="optional"/>
      <!--Nuovo campo OPtoolID -->
    </xs:complexType>
  </xs:element>
  <xs:element name="MEASUREMENT">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="ELENCO_2">
          <xs:attribute name="MEAS_DESCR" type="STRINGA_GENER" use="optional"/>
          <xs:attribute name="MEASURE" type="ELENCO_4" use="required"/>
          <xs:attribute name="VALUE" type="STRINGA_GENER" use="required"/>
          <xs:attribute name="UM" type="ELENCO_3" use="required"/>
          <xs:attribute name="UPPER_TOLERANCE" type="DECIMAL" use="optional"/>
          <xs:attribute name="LOWER_TOLERANCE" type="DECIMAL" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <!--*************************** -->
  <!--*************************** -->
  <!-- SEZIONE PER LA GESTIONE DELLE CENTRALINE -->
  <!--*************************** -->
  <!--*************************** -->
  <xs:element name="ECU_SECTION">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="SINGLE_ECU_DATA" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="SINGLE_ECU_DATA">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ERROR_LIST" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="RLI_FIELDS_LIST" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="OPCODE" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="ECU_TYPE" type="ELENCO_7" use="required"/>
      <xs:attribute name="ECU_HW_Number" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="ECU_HW_VersNumber" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="ECU_SW_Number" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="ECU_SW_VersNumber" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="ECU_Serial_Number" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="ECU_Part_Number" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="ECU_Progr_Date" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="ECU_RESULT" type="ELENCO_2" use="required"/>
      <xs:attribute name="ECU_TOOL_ID" type="STRINGA_GENER" use="optional"/>
      <xs:attribute name="OPERATOR" type="STRINGA_GENER" use="optional"/>
      <!--Nuovo campo TRY_ID. Da verificare l'aggiunta la gestione della ECU_Single_Op_Result -->
      <xs:attribute name="TRY_ID" type="INTEGER" use="optional"/>
      <!--Nuovo campo TRY_ID. Da verificare l'aggiunta la gestione della ECU_Single_Op_Result -->
      <xs:attribute name="ECU_SW_Calibr_Number" type="STRINGA_GENER" use="optional"/>
      <xs:attribute name="ECU_DOTE" type="STRINGA_GENER" use="optional"/>
      <xs:attribute name="ECU_Supplier" type="STRINGA_GENER" use="optional"/>
      <xs:attribute name="ECU_Unlock_KEY" type="STRINGA_GENER" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="RLI_FIELDS_LIST">
    <xs:complexType>
      <xs:attribute name="RLI_FIELD" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="RLI_DESCR" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="RLI_VALUE" type="STRINGA_GENER" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ERROR_LIST">
    <xs:complexType>
      <xs:attribute name="STATISTIC_CODE" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="STATISTIC_DESCRIPTION" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="STATISTIC_VALUE" type="STRINGA_GENER" use="required"/>
    </xs:complexType>
  </xs:element>
  <!--*************************** -->
  <!--*************************** -->
  <!-- SEZIONE PER DATI PROGRAMMAZIONE CHIAVI CODE2 -->
  <!--*************************** -->
  <!--*************************** -->
  <xs:element name="KEY_CODE_SECTION">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="SINGLE_KEY_DATA" minOccurs="0" maxOccurs="4"/>
      </xs:sequence>
      <xs:attribute name="OPCODE" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="OP_RESULT" type="ELENCO_2" use="required"/>
      <xs:attribute name="PROGDATE" type="DATETIME" use="required"/>
      <xs:attribute name="PDF_DATA" type="STRINGA_GENER" use="optional"/>
      <xs:attribute name="PASSWORD" type="STRINGA_GENER" use="optional"/>
      <xs:attribute name="IMMOCODE" type="STRINGA_ALN_10" use="optional"/>
      <xs:attribute name="KEY_AES_CODE" type="STRINGA_GENER" use="optional"/>
      <xs:attribute name="OPERATOR" type="STRINGA_GENER" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="SINGLE_KEY_DATA">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="ELENCO_2">
          <xs:attribute name="KEY_ID" type="STRINGA_GENER" use="required"/>
          <xs:attribute name="FAPP" type="STRINGA_GENER" use="required"/>
          <xs:attribute name="SEED" type="STRINGA_GENER" use="required"/>
          <xs:attribute name="IDTX" type="STRINGA_GENER" use="required"/>
          <xs:attribute name="OPERATOR" type="STRINGA_GENER" use="optional"/>
          <!--Nuovo campo OPtoolID -->
          <xs:attribute name="OPTOOL_ID" type="STRINGA_GENER" use="optional"/>
          <!--Nuovo campo OPtoolID -->
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <!--*************************** -->
  <!--*************************** -->
  <!-- SEZIONE PER TRACCIATURA DATI GENERICI -->
  <!--*************************** -->
  <!--*************************** -->
  <xs:element name="GENERIC_DATA_COLLECTION">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="COLLECTED_ELEMENT" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="COLLECTED_ELEMENT">
    <xs:complexType>
      <!--Nuovo sezione ERROR LIST -->
      <!--<xs:sequence>-->
      <!--<xs:element ref="ERROR_LIST" minOccurs="0" maxOccurs="unbounded"/>-->
      <!--</xs:sequence>-->
      <!--Nuovo sezione ERROR LIST -->
      <xs:simpleContent>
        <xs:extension base="ELENCO_2">
          <xs:attribute name="OPCODE" type="STRINGA_GENER" use="required"/>
          <xs:attribute name="CODING" type="STRINGA_GENER" use="required"/>
          <xs:attribute name="CODING_DESCRIPTION" type="STRINGA_GENER" use="required"/>
          <!--Nuovo campo OPerator -->
          <xs:attribute name="OPERATOR" type="STRINGA_GENER" use="optional"/>
          <!--Nuovo campo OPerator -->
          <!--Nuovo campo OPtoolID -->
          <xs:attribute name="OPTOOL_ID" type="STRINGA_GENER" use="optional"/>
          <!--Nuovo campo OPtoolID -->
          <!--Nuovo campo UM -->
          <xs:attribute name="UM" type="ELENCO_3" use="optional"/>
          <!--Nuovo campo UM. Il cui valore C( presente nel campo "Coding" -->
          <!--Nuovo campo Value -->
          <xs:attribute name="VALUE" type="TRUE-FALSE" use="optional"/>
          <!--Nuovo campo Value. Serve a identificare se 	nel campo "Coding" il valore ha un UM correlato o No -->
          <!--Nuovo campo TRY_ID -->
          <xs:attribute name="TRY_ID" type="INTEGER" use="optional"/>
          <!--Nuovo campo TRY_ID -->
          <!--Nuova gestione dei componenti ERROR LIST -->
          <xs:attribute name="STATISTIC_CODE" type="STRINGA_GENER" use="optional"/>
          <xs:attribute name="STATISTIC_DESCRIPTION" type="STRINGA_GENER" use="optional"/>
          <xs:attribute name="STATISTIC_VALUE" type="STRINGA_GENER" use="optional"/>
          <!--Nuova gestione dei componenti ERROR LIST -->
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <!--*************************** -->
  <!--*************************** -->
  <!--La sezione riporta eventuali codici TESIS che sono stati aperti su sistemi di qualitC  di plant e che devono essere comunicati verso MES-->
  <!--Per ogni codice tesis sono riportati tutti i dati significativi-->
  <!--SEZIONE ESCLUDIBILE A LIVELLO DI SINGOLO STREAM DI COMUNICAZIONE-->
  <!--*************************** -->
  <!--*************************** -->
  <xs:element name="TESIS_COMMUNICATION">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="TESIS_DETAIL" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="TESIS_DETAIL">
    <!-- Per un tesis che dovesse essere stato aperto con molteplicitC  >1 viene ripetuto l'elemento, per un tesis che dovesse essere aperto e chiuso le operazioni di apertura e chiusura devono essere comunicate dal sistema di qualitC  nel corretto ordine-->
    <xs:complexType>
      <xs:attribute name="TESIS_COMPLETE_CODE" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="TEST_TYPE" type="STRINGA_GENER" use="optional"/>
      <xs:attribute name="CRITICITY" type="ELENCO_9" use="optional"/>
      <xs:attribute name="QI4_SOURCE" type="ELENCO_8" use="optional"/>
      <xs:attribute name="TESIS_UPDATION_STATUS" type="TRUE-FALSE" use="required"/>
      <xs:attribute name="TESIS_OPEN_DATE" type="DATETIME" use="optional"/>
      <xs:attribute name="TESIS_OPEN_OPERATOR" type="STRINGA_GENER" use="optional"/>
      <xs:attribute name="TESIS_CLOSE_DATE" type="DATETIME" use="optional"/>
      <xs:attribute name="TESIS_CLOSE_OPERATOR" type="STRINGA_GENER" use="optional"/>
      <!-- OPEN=1 RESOLVED=0-->
      <!-- Validato solo in caso si stia comunicando l'avvenuta apertura del difetto-->
      <!-- Validato solo in caso si stia comunicando l'avvenuta apertura del difetto-->
      <!-- Validato solo in caso si stia comunicando l'avvenuta chiusura del difetto -->
      <!-- Validato solo in caso si stia comunicando l'avvenuta chiusura del difetto -->
      <!--Nuovo campo OPtoolID_OPEN -->
      <xs:attribute name="OPTOOL_ID_OPEN" type="STRINGA_GENER" use="optional"/>
      <!--Nuovo campo OPtoolID_OPEN -->
      <!--Nuovo campo OPtoolID_CLOSE -->
      <xs:attribute name="OPTOOL_ID_CLOSE" type="STRINGA_GENER" use="optional"/>
      <!--Nuovo campo OPtoolID_CLOSE -->
      <xs:attribute name="CONDITION" type="STRINGA_GENER" use="optional"/>
      <xs:attribute name="NOTE" type="STRINGA_GENER" use="optional"/>
    </xs:complexType>
  </xs:element>
  <!--*************************** -->
  <!--*************************** -->
  <!-- SEZIONE PER I DATI DI PIAZZALE -->
  <!--*************************** -->
  <!--*************************** -->
  <xs:element name="COMPOUND_SECTION">
    <xs:complexType>
      <xs:attribute name="COMPOUND_CODE" type="STRINGA_NUM_5" use="required"/>
      <xs:attribute name="TRANSACTION_DATE" type="DATETIME" use="required"/>
      <xs:attribute name="SYSDATE" type="DATETIME" use="required"/>
    </xs:complexType>
  </xs:element>
  <!--*************************** -->
  <!--*************************** -->
  <!-- SEZIONE PER LA GESTIONE DELL' ASSORBIMENTO MATERIALI DA PTL -->
  <!--*************************** -->
  <!--*************************** -->
  <xs:element name="PTL_ACCOUNTING">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ACCOUNTED_SEQUENCE" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="VIRTUAL_AREA" type="STRINGA_GENER" use="required"/>
      <xs:attribute name="RACKID" type="STRINGA_NUM_4" use="optional"/>
      <xs:attribute name="TRACT_ID" type="INTEGER" use="optional"/>
      <xs:attribute name="IS_CLOSED" type="TRUE-FALSE" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="ACCOUNTED_SEQUENCE">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ACCOUNTED_MATERIAL" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="SEQUENCE" type="INTEGER_5" use="required"/>
      <xs:attribute name="OPERATION_TS" type="DATETIME" use="required"/>
    </xs:complexType>
